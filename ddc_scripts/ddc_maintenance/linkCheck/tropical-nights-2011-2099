        function c3sView(type) {
            if (type=='details') {
                location.hash = '#details';
                $('.c3s_overview').css('display','none');
                $('.c3s_details').attr('style','display:block;margin-top:-60px;');
                window.cds_toolbox.runApp('toolbox-app-details', c3s_details_url, c3s_details_params);
            } else {
                location.hash = '';
                $('.c3s_details').attr('style','display:none;');
                $('.c3s_overview').attr('style','display:block;');
            }
        }
